---
import BaseLayout from '../../layouts/BaseLayout.astro';
import IndividualProduct from "../../components/IndividualProduct.astro";
import ProductLayout from "../../layouts/ProductLayout.astro";

export async function getStaticPaths() {
  return [
    { params: { tag: "giftBags" } },
    { params: { tag: "giftBoxes" } },
    { params: { tag: "foldingGiftBoxes" } },
    { params: { tag: "tinBoxes" } },
  ];
}

const { tag } = Astro.params;
---
<ProductLayout pageTitle={tag}>
    <IndividualProduct folder={tag} />

</ProductLayout>