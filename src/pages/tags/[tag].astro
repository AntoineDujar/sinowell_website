---
import BaseLayout from '../../layouts/BaseLayout.astro';
import IndividualProduct from "../../components/IndividualProduct.astro";
import ProductLayout from "../../layouts/ProductLayout.astro";

export async function getStaticPaths() {
  return [
    { params: { tag: "giftBags"}, props: {pageTitle: "Folding gift bags"} },
    { params: { tag: "giftBoxes"}, props: {pageTitle: "Gift boxes" } },
    { params: { tag: "foldingGiftBoxes"}, props: {pageTitle: "Folding gift boxes" } },
    { params: { tag: "tinBoxes"}, props: {pageTitle: "Tin boxes" } },
  ];
}

const { tag } = Astro.params;
const { pageTitle } = Astro.props;
---
<ProductLayout pageTitle={pageTitle}>
    <IndividualProduct folder={tag} />

</ProductLayout>